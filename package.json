{
  "name": "a-just",
  "version": "0.0.1",
  "engines": {
    "node": "12.18.4"
  },
  "scripts": {
    "db:build": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml build",
    "db:start": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml up -d",
    "test": "npm run test:api",
    "test:api": "docker-compose -p test-api -f docker-compose.test.yml run --rm api; exitcode=$?; docker-compose -p test-api -f docker-compose.test.yml down; exit ${exitcode}",
    "build": "npm run build:api && npm run build:front",
    "build:api": "cd api && npm i && npm run build && cd ..",
    "build:front": "cd front && npm i && npm install --dev && npm run build && mv dist ../api/dist/front && cd ..",
    "start:production": "cd api && npm run start:production"
  },
  "dependencies": {
    "@hubspot/api-client": "^4.0.0",
    "@sentry/node": "^6.2.3",
    "@sentry/tracing": "^6.2.3",
    "async": "^3.2.0",
    "awesome-phonenumber": "^2.44.0",
    "aws-sdk": "^2.960.0",
    "axios": "^0.21.1",
    "base64-stream": "^1.0.0",
    "bcryptjs": "^2.4.3",
    "config": "^3.3.2",
    "cron": "^1.8.2",
    "crypto": "^1.0.1",
    "csv-parse": "^4.14.0",
    "dotenv": "^8.2.0",
    "firebase-admin": "^9.5.0",
    "fs": "0.0.1-security",
    "geolib": "^3.3.1",
    "html-entities": "^1.3.1",
    "jimp": "^0.16.1",
    "jsonwebtoken": "^8.5.1",
    "koa-body": "^4.0.6",
    "koa-ejs": "^4.3.0",
    "koa-smart": "^2.2.0",
    "lodash": "^4.17.20",
    "logzio-nodejs": "^2.0.2",
    "mime": "^2.5.2",
    "moment": "^2.29.1",
    "momentjs": "^2.0.0",
    "node-sass": "^6.0.1",
    "path": "^0.12.7",
    "pdfkit": "^0.12.1",
    "pg": "^8.5.1",
    "qrcode": "^1.4.4",
    "querystring": "^0.2.0",
    "sequelize": "^6.3.5",
    "sequelize-cli": "^6.2.0",
    "sequelize-transforms": "^2.0.0",
    "sib-api-v3-sdk": "^8.0.2",
    "slugify": "^1.5.3",
    "socket.io": "^4.1.2",
    "stream": "^0.0.2",
    "string-strip-html": "^6.3.0",
    "stripe": "^8.132.0",
    "winston": "^3.3.3",
    "winston-logzio": "^5.1.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-3": "^6.17.0",
    "eslint": "^7.13.0",
    "expect": "^26.6.2",
    "mocha": "^8.2.1",
    "nodemon": "^2.0.6",
    "prettier-eslint": "^11.0.0"
  }
}
