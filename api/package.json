    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-import-meta": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.25.9",
    "@babel/preset-env": "^7.26.0",
    "@babel/runtime": "^7.26.0",
    "@hubspot/api-client": "^12.0.1",
    "@koa/cors": "^5.0.0",
    "@sentry/node": "^8.35.0",
    "@sentry/profiling-node": "^8.54.0",
    "@sentry/utils": "^8.35.0",
    "base64-stream": "^1.0.0",
    "bcryptjs": "^2.4.3",
    "chai": "^4.5.0",
    "config": "^3.3.12",
    "cron": "^3.1.8",
    "crypto": "^1.0.1",
    "crypto-js": "^4.2.0",
    "csv-parse": "^5.5.6",
    "csv-writer": "^1.6.0",
    "date-fns": "^4.1.0",
    "deep-diff": "^1.0.2",
    "dotenv": "^16.4.5",
    "fast-deep-equal": "^3.1.3",
    "form-data": "^4.0.1",
    "http-auth": "^4.2.1",
    "jsonwebtoken": "^9.0.2",
    "koa-body": "^6.0.1",
    "koa-csp": "^1.2.6",
    "koa-helmet": "^8.0.1",
    "koa-ip": "^2.1.3",
    "koa-redis": "^4.0.1",
    "koa-session": "^6.4.0",
    "koa-smart": "^4.1.1",
