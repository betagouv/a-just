    "stream": "^0.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/node": "^7.26.0",
    "@babel/preset-stage-3": "^7.8.3",
    "@babel/register": "^7.25.9",
    "axios": "^1.9.0",
    "babel-eslint": "^10.1.0",
