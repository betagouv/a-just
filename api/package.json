{
  "name": "a-just-api",
  "version": "0.0.1",
  "scripts": {
    "install": "npm install --save-dev @babel/node && ",
    "start": "TZ='Europe/Paris' nodemon --exec babel-node src/index.js",
    "start:production": "TZ='Europe/Paris' node --optimize_for_size --gc_interval=100 --max-old-space-size=2048 dist/index.js",
    "build": "npm run rm-build-prod && npm run build-prod",
    "build:test": "npm run build-prod",
    "build:documentation": "jsdoc -r src -d documentation",
    "build-prod": "babel src --out-dir dist --copy-files",
    "rm-build-prod": "rm -rf dist",
    "test": "NODE_ENV=test node_modules/.bin/nyc --reporter=lcov node_modules/.bin/mocha"
  },
  "dependencies": {
    "axios": "^0.21.4",
    "babel": "^6.23.0",
    "base64-stream": "^1.0.0",
    "bcryptjs": "^2.4.3",
    "config": "^3.3.2",
    "connect-datadog": "^0.0.9",
    "cron": "^1.8.2",
    "crypto": "^1.0.1",
    "csv-parse": "^4.14.0",
    "csv-writer": "^1.6.0",
    "date-fns": "^2.29.1",
    "dd-trace": "^3.13.2",
    "dotenv": "^8.2.0",
    "form-data": "^4.0.0",
    "jsonwebtoken": "9.0.0",
    "koa-body": "^4.0.6",
    "koa-logger-winston": "^0.0.2",
    "koa-smart": "^2.2.0",
    "lodash": "^4.17.20",
    "mime": "^2.5.2",
    "nyc": "^15.1.0",
    "path": "^0.12.7",
    "pg": "^8.5.1",
    "sequelize": "^6.3.5",
    "sequelize-cli": "^6.2.0",
    "sequelize-transforms": "^2.0.0",
    "sib-api-v3-sdk": "^8.5.0",
    "stream": "^0.0.2",
    "winston": "^3.8.2",
    "winston-logzio": "^5.1.2",
    "winston-sentry-log": "^1.0.26"
  },
  "devDependencies": {
    "@babel/node": "^7.20.7",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-3": "^6.17.0",
    "babel-register": "^6.26.0",
    "chai": "^4.3.4",
    "eslint": "^7.13.0",
    "expect": "^26.6.2",
    "jsdoc": "^4.0.0",
    "mocha": "^8.4.0",
    "nock": "^13.2.1",
    "nodemon": "^2.0.20",
    "prettier-eslint": "^11.0.0"
  }
}
