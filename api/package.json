{
  "name": "a-just-api",
  "version": "0.0.1",
  "scripts": {
    "start": "TZ='Europe/Paris' nodemon --exec babel-node src/index.js",
    "start:production": "cd dist && TZ='Europe/Paris' node --optimize_for_size --gc_interval=100 --max-old-space-size=2048 index.js",
    "build": "npm run rm-build-prod && npm run build-prod",
    "build:test": "npm run build-prod",
    "build:documentation": "jsdoc -r src -d documentation",
    "build-prod": "babel src --out-dir dist --copy-files && babel config --out-dir dist/config --copy-files",
    "rm-build-prod": "rm -rf dist",
    "test": "NODE_ENV=test mocha --config .mocharc.yaml"
  },
  "dependencies": {
    "@babel/cli": "^7.25.9",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.25.9",
