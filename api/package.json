{
  "name": "a-just-api",
  "version": "0.0.1",
  "scripts": {
    "start": "TZ='Europe/Paris' nodemon --exec babel-node src/index.js",
    "start:production": "TZ='Europe/Paris' node --optimize_for_size --gc_interval=100 --max-old-space-size=2048 dist/index.js",
    "build": "npm run rm-build-prod && npm run build-prod",
    "build:test": "npm run build-prod",
    "build:documentation": "jsdoc -r src -d documentation",
    "build-prod": "babel src --out-dir dist --copy-files",
    "rm-build-prod": "rm -rf dist",
    "test": "NODE_ENV=test node_modules/.bin/nyc --reporter=lcov node_modules/.bin/mocha"
  },
  "dependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.20.2",
    "@babel/runtime": "^7.21.0",
    "@babel/runtime-corejs2": "^7.21.0",
    "@sentry/node": "^7.52.1",
    "base64-stream": "^1.0.0",
    "bcryptjs": "^2.4.3",
    "config": "^3.3.2",
    "connect-datadog": "^0.0.9",
    "cron": "^1.8.2",
    "crypto": "^1.0.1",
    "csv-parse": "^4.14.0",
    "csv-writer": "^1.6.0",
    "date-fns": "^2.29.1",
    "dd-trace": "^3.13.2",
    "dotenv": "^8.2.0",
    "form-data": "^4.0.0",
    "jsonwebtoken": "9.0.0",
    "koa-body": "^4.0.6",
    "koa-csp": "^1.2.1",
    "koa-logger-winston": "^0.0.2",
    "koa-smart": "^2.2.0",
    "koa-sslify": "^5.0.1",
    "lodash": "^4.17.20",
    "mime": "^2.5.2",
    "nyc": "^15.1.0",
    "path": "^0.12.7",
    "pg": "^8.5.1",
    "sequelize": "^6.3.5",
    "sequelize-cli": "^6.2.0",
    "sequelize-transforms": "^2.0.0",
    "sib-api-v3-sdk": "^8.5.0",
    "stream": "^0.0.2",
    "winston": "^3.8.2",
    "winston-logzio": "^5.1.2",
    "winston-sentry-log": "^1.0.26",
    "winston-transport-sentry-node": "^2.7.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/node": "^7.0.0",
    "@babel/preset-stage-3": "^7.8.3",
    "@babel/register": "^7.0.0",
    "axios": "^1.4.0",
    "babel-eslint": "^10.1.0",
    "chai": "^4.3.4",
    "eslint": "^7.13.0",
    "expect": "^26.6.2",
    "jsdoc": "^4.0.0",
    "mocha": "^8.4.0",
    "nock": "^13.2.1",
    "nodemon": "^2.0.20",
    "prettier-eslint": "^11.0.0",
    "sinon": "^15.0.4"
  }
}
