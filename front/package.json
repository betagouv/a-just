{
  "name": "a-just-front",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --configuration=${NODE_ENV}${NODE_ENV_EXT}",
    "start-e2e": "ng serve --host 0.0.0.0 --port 4200",
    "build:stats": "ng build --configuration=production --stats-json",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "compodoc": "npx compodoc -p tsconfig.doc.json -s -w --port 8181",
    "analyse:build": "npm run build:stats && npx webpack-bundle-analyzer dist/stats.json",
    "e2e": "npx cypress open",
    "format": "node format-angular-templates.js && prettier --write \"src/**/*.{ts,scss,css,js,json}\"",
    "format:html": "node format-angular-templates.js",
    "format:check": "prettier --check \"src/**/*.{ts,html,scss,css,js,json}\"",
    "format:angular": "node format-angular-templates.js",
    "compodoc:build": "compodoc -p tsconfig.doc.json",
    "compodoc:build-and-serve": "compodoc -p tsconfig.doc.json -s",
    "compodoc:serve": "compodoc -s"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.1.2",
    "@angular/cdk": "^20.1.2",
